alias clear="printf '\033c'; seq 1 $(tput cols) | sort -R | spark | lolcat"
alias cls=clear
alias v=nvim
alias nvimrc='v ~/.config/nvim/init.vim'
alias zshrc='v ~/.zshrc'
alias aliasrc='v ~/.config/shell/aliasrc'
alias py=python3
alias ipy=ipython3
alias jnb='jupyter notebook --ip 192.168.1.103 --port 8888'
alias gitcc='git config --global credential.helper cache && echo Git creds saved'

